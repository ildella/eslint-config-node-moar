{
  "name": "eslint-config-node-moar",
  "version": "1.0.4",
  "description": "Moar: the linter config for people who does not want to be standard. Core Package.",
  "packageManager": "yarn@4.1.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ildella/eslint-config-node-moar"
  },
  "author": {
    "name": "Daniele Dellafiore"
  },
  "contributors": [],
  "license": "MIT",
  "engines": {
    "node": ">=16.10"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "nodejs"
  ],
  "scripts": {
    "audit": "yarn npm audit",
    "lint": "eslint . --max-warnings 0",
    "lint.flat": "eslint . --max-warnings 0 --config eslint.config.js",
    "lint.print": "eslint --print-config .eslintrc.js > all-config.json",
    "release": "dev-github-npm-release-and-publish.sh"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.0.1",
    "eslint": "8.56.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-sonarjs": "0.24.0"
  },
  "peerDependencies": {
    "eslint": ">=8.23.0",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-promise": ">=5.0.0",
    "eslint-plugin-security": ">=1.5.0 <2.0",
    "eslint-plugin-sonarjs": ">=0.5.0"
  }
}
