{
  "name": "eslint-config-node-moar",
  "version": "1.0.0",
  "description": "The linter config for people who do not want to be standard.",
  "packageManager": "yarn@4.1.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ildella/eslint-config-node-moar"
  },
  "author": {
    "name": "Daniele Dellafiore"
  },
  "contributors": [],
  "license": "MIT",
  "engines": {
    "node": ">=16.10"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "nodejs"
  ],
  "scripts": {
    "audit": "yarn npm audit",
    "lint": "eslint . --max-warnings 0 --config src/config.js",
    "print": "eslint --print-config src/config.js > all-config.js"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-sonarjs": "0.24.0"
  },
  "peerDependencies": {
    "eslint": ">=8.23.0",
    "eslint-plugin-n": ">=16.0.0",
    "eslint-plugin-promise": ">=5.0.0",
    "eslint-plugin-security": ">=1.5.0 <2.0.0",
    "eslint-plugin-sonarjs": ">=0.5.0"
  }
}
