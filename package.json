{
  "name": "eslint-config-node-moar",
  "version": "0.10.0",
  "description": "The linter config and plugins for people who does not want to be standard but wants moar",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ildella/eslint-config-node-moar"
  },
  "author": {
    "name": "Daniele Dellafiore"
  },
  "contributors": [],
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=16.10"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "nodejs"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0 --config src/config.js",
