  },
  "peerDependencies": {
    "eslint": ">=6.8.0",
    "eslint-plugin-node": ">=11.0.0",
    "eslint-plugin-security": ">=1.4.0",
    "eslint-plugin-sonarjs": ">=0.5.0"
  }
}
