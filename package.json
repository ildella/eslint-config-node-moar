{
  "name": "eslint-config-node-opinionated",
  "version": "0.4.0",
  "description": "The linter config and plugins I use in Node.js projects.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ildella/eslint-config-node-opinionated"
  },
  "author": {
    "name": "Daniele Dellafiore",
    "email": "daniele@dellafiore.net"
  },
  "contributors": [],
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "nodejs"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0 --config src/config.js",
    "print": "eslint --print-config src/config.js >> all-config.js",
    "bug.setup": "git bug bridge configure --target=github --url=https://github.com/ildella/eslint-config-node-opinionated"
  },
  "peerDependencies": {
    "eslint": ">=6.8.0",
    "eslint-plugin-node": ">=11.0.0",
    "eslint-plugin-security": ">=1.4.0",
    "eslint-plugin-sonarjs": ">=0.5.0"
  }
}
